{
  "name": "@dev-mcp/prompt-server",
  "version": "1.0.0",
  "description": "MCP Server providing curated prompts for AI-powered development workflows",
  "main": "build/server.js",
  "type": "module",
  "bin": {
    "dev-mcp": "./build/server.js"
  },
  "scripts": {
    "build": "tsc && chmod +x build/server.js",
    "start": "node build/server.js",
    "dev": "tsc --watch",
    "test": "npm run build && node --test build/test/*.js",
    "inspector": "npx @modelcontextprotocol/inspector build/server.js"
  },
  "keywords": [
    "mcp",
    "claude",
    "prompts",
    "ai",
    "development",
    "ui",
    "ux",
    "templates"
  ],
  "author": "dev-mcp team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dev-mcp/prompt-server.git"
  },
  "mcp": {
    "server": {
      "command": "node",
      "args": [
        "./build/server.js"
      ]
    }
  }
}
